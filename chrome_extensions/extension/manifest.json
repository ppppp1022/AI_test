{
  "manifest_version": 3,
  "name": "Python Communication Toggle",
  "version": "1.0",
  "permissions": ["tabs", "nativeMessaging", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extension Toggle"
  },
  "background": {
    "service_worker": "background.js"
  }
}